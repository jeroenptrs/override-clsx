  "module": "dist/override-clsx.esm.js",
  "devDependencies": {
    "dtslint": "3.6.12",
    "husky": "4.2.5",
    "shx": "0.3.2",
    "tsdx": "0.13.2",
    "tslib": "2.0.0",
    "typescript": "3.9.5"
  },
  "dependencies": {
